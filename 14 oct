2 numers define in one array 
#include <iostream>
using namespace std;
int main() {
  int arr[] = {1 , 2, 3, 1, 3, 4, 2};
  int n = 7;
   int count = 0;
   for (int i = 0; i<n; i++){
       for(int j= i + 1; j<n ;j++ ){
           if (arr[i] == arr[j]){
               count++;
           }
       }
   }
cout<<count<<endl;
    return 0;
}



find the number in the array.  (linear sufficient)
#include <iostream>
using namespace std;
int main(){
    int arr[7] = {2, 3, 4, 1, -5, 7};
    int target = 7;
   bool flag = false;
    for (int i=0; i<6;i++){
        if(arr[i]==target){
            flag = true;
        }
    }
   if(flag == true)  cout<<"found"<<endl;
   else cout<<"Not found"<<endl;
    
    
    return 0;
}



find the occurence number
#include <iostream>
using namespace std;

int main() {
    int a[] = {2,3,1,4,5,1,1,6}, n = 8, m = 1;
    for(int i = 0; i < n; i++)
        if(a[i] == m) { cout << i; return 0; }
    cout << "-1";
}





